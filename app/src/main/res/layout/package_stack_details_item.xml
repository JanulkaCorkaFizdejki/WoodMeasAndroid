<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayoutPackageStackDetails"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="2dp"
    android:background="@drawable/rectangle_white_bg"
    android:paddingTop="4dp"
    android:paddingBottom="4dp">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textViewStackPackageDetailsId"
            android:layout_width="16dp"
            android:layout_height="wrap_content"
            android:text="@string/default_number"
            android:textSize="6sp"
            tools:ignore="SmallSp" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <LinearLayout
            android:id="@+id/linearLayout11"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            tools:ignore="UseCompoundDrawables">

            <ImageView
                android:id="@+id/imageViewPackageStackDetailsTreeIco"
                android:layout_width="8dp"
                android:layout_height="8dp"
                app:srcCompat="@drawable/ic_tree_leafy_green_8"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/textViewPackageStackDetailsTreeName"
                android:layout_width="42dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginLeft="2dp"
                android:text="@string/default_number"
                android:textColor="@color/wm_green_medium"
                android:textSize="7sp"
                android:textStyle="bold"
                tools:ignore="SmallSp" />
        </LinearLayout>

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/textViewPackageStackDetailsLength"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/default_number"
            android:textColor="@color/wm_gray_dark"
            android:textSize="8sp"
            tools:ignore="SmallSp" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/textViewPackageStackDetailsWidth"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/default_number"
            android:textColor="@color/wm_gray_dark"
            android:textSize="8sp"
            tools:ignore="SmallSp" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/textViewPackageStackDetailsHeight"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/default_number"
            android:textColor="@color/wm_gray_dark"
            android:textSize="8sp"
            tools:ignore="SmallSp" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <ImageView
            android:id="@+id/imageViewPackageStackDetailsCrossIco"
            android:layout_width="8dp"
            android:layout_height="8dp"
            app:srcCompat="@drawable/ic_cross_on_8"
            tools:ignore="ContentDescription" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/textViewStackPackageDetailsCubic"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/default_number"
            android:textColor="@color/wm_gray_dark"
            android:textSize="8sp"
            android:textStyle="bold"
            tools:ignore="SmallSp" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <ImageButton
            android:id="@+id/imageButtonPackageStackDetailsDelete"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/oval_white_smoke"
            app:srcCompat="@drawable/ic_cancel_10"
            tools:ignore="ContentDescription" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>